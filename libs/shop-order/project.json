{
  "name": "shop-order",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shop-order/src",
  "prefix": "wsv2",
  "tags": [
   "01app-shop-order-lib",
   "type:01-lib"
   ],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/shop-order/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shop-order/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shop-order/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shop-order/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shop-order/**/*.ts",
          "libs/shop-order/**/*.html"
        ]
      }
    }
  }
}

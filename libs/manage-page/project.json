{
  "name": "manage-page",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/manage-page/src",
  "prefix": "wsv2",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/manage-page/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/manage-page/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/manage-page/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/manage-page/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/manage-page/**/*.ts",
          "libs/manage-page/**/*.html"
        ]
      }
    }
  },
  "tags": [
    "scope:01app-manage-page-lib",
    "type:01-lib"
 ]
}
